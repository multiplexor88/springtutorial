<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

    <bean id="foo" class="spring.core.autowiring.Foo"/>
    <bean id="bar1" class="spring.core.autowiring.Bar"/>
    <bean id="byName" class="spring.core.autowiring.Target" autowire="byName" lazy-init="true"/>
    <bean id="byType" class="spring.core.autowiring.Target" autowire="byType" lazy-init="true"/>
    <bean id="constructor" class="spring.core.autowiring.Target" autowire="constructor" lazy-init="true"/>
</beans>